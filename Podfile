# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'
post_install do |installer|
  installer.pods_project.targets.each do |target|
    target.build_configurations.each do |config|
      config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '13.0'
    end
  end
end

target 'Phoneado' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  
  # Pods for Phoneado
  #pod 'PinCodeView'
  pod 'SDWebImage'
  pod 'IQKeyboardManager'
  pod 'SVPinView'
  pod 'ADCountryPicker', '~> 2.1.0'
  #pod 'PinCodeView/Core'
  #pod 'PinCodeView/RxSwift'
  pod 'Alamofire', '~> 5.5.0'
  pod 'SDWebImage'
  pod 'OTPFieldView'
 # pod 'AXPhotoViewer/SDWebImage', git: 'https://github.com/RyutaroYako/AXPhotoViewer', branch: 'master'
  pod 'SideMenu'
  pod 'Alamofire'
  pod 'PanSlip'
  pod 'MBProgressHUD'
  pod 'Socket.IO-Client-Swift', '~> 15.2.0'
  pod 'PhoneNumberKit'
  pod 'TwilioVideo'
  pod 'TwilioVoice'
  pod 'GrowingTextView'
  pod 'SwiftyJSON'
  pod 'Reachability', '~> 3.1.1'
  pod 'Firebase/Auth', '~> 9.2.0'
  pod 'Firebase/Analytics', '~> 9.2.0'
  pod 'Firebase/Messaging', '~> 9.2.0'
  pod 'Firebase/Crashlytics', '~> 9.2.0'
  pod 'OTPFieldView'
  pod 'CropViewController'
  pod 'SVProgressHUD'
  pod 'GoogleMaps'
end
